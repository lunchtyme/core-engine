<!-- Footer snippet -->
<tr id="footer-snippet"
  style="Margin:0;padding:0;display:block;width:100%;Margin:1rem 0;max-width:400px;text-align:center;background-color:var(--footer-Bg-color, #8a8787);">
  <td style="Margin:0;padding:0;">
    <table id="footer-snippet-wrapper" width="100%" style="Margin:0;border-spacing: 0;padding: 1rem;">

      <!-- Multiple text snippet -->
      <tr id="multiple-text-snippet" style="Margin:0;padding:0;display: block;">
        <td style="Margin:0;padding:0;">
          <table id="multiple-text-snippet-wrapper" style="Margin:0;padding:0;border-spacing:0;">
            <tr class="text-snippet-row" style="Margin:0;padding:0;">
              <td style="Margin:0;padding:0;">
                <p class="footer-text space-b-1"
                  style="Margin:0;padding:0;margin-bottom:0.5rem;color:var(--para-text-color, #FFFFFF);font-size:1rem;">
